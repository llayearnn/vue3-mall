{
  "$schema": "https://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "semi": false,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "auto",
  "trailingComma": "all",
  "quoteProps": "as-needed",
  "tabWidth": 2,
  "arrowParens": "avoid",
  "vueIndentScriptAndStyle": false
}
